class NaiveBayesInputsStub {
    static getAlegriaTextTrainingSet(){
        return [
            'estas muy grande',
            'haces todo bien',
            'eres increible',
            'muy bien hecho',
            'muchas gracias',
            'eres magnifica',
            'eres bueno',
            'eres una buena persona',
            'te quiero',
            'lo hiciste bien',
            'quiero estar contigo',
            'eres una ganadora'
        ]
    }


    static getTristezaTextTrainingSet (){
        return [
            'no sirves para nada',
            'lo hiciste mal',
            'eres una mala persona',
            'eres malvado',
            'mal hecho',
            'no hay nada que agradecer',
            'eres malo',
            'no te quiero',
            'haces todo mal',
            'ya no quiero estar contigo',
            'no lo hiciste bien',
            'eres un perdedor'
        ]
    }

    static getSiDisfrutaTableTrainningSet () {
        const dataSet = [
            'nublado,caluroso,alta,debil',
            'lluvioso,templado,alta,debil',
            'lluvioso,frio,normal,debil',
            'nublado,frio,normal,fuerte',
            'soleado,frio,normal,debil',
            'lluvioso,templado,normal,debil',
            'soleado,templado,normal,fuerte',
            'nublado,templado,alta,fuerte',
            'nublado,caluroso,normal,debil']

        let arraySiDisfruta = []

        dataSet.forEach(rec => {
            arraySiDisfruta.push(rec.split(',').slice());
        });

        return arraySiDisfruta
    }

    static getNoDisfrutaTableTrainningSet () {
        return [
            'soleado,caluroso,alta,debil',
            'soleado,caluroso,alta,fuerte',
            'lluvioso,fr√≠o,normal,fuerte',
            'soleado,templado,alta,debil',
            'lluvioso,templado,alta,fuerte']
    }
}

module.exports = NaiveBayesInputsStub
